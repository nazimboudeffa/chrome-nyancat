{
"name": "Nyan Cat for YouTube",
"description": "Transform your YouTube progress bar into a cute Nyan Cat",
"author": "Nazim Boudeffa",
"manifest_version": 2,
"version": "1.0.0",
"web_accessible_resources": [ "nyan.gif" ],
"browser_action": {
  "default_icon": "icons/32.png",
  "default_popup": "nyan.html"
},
"content_scripts": [
    {
      "css": [ "nyan.css" ],
      "matches": [ "*://*.youtube.com/*" ]
    }
  ],
"icons": {
  "16": "icons/16.png",
  "32": "icons/32.png",
  "48": "icons/48.png",
  "128": "icons/128.png"
 }
}
